{
    "name": "hello",
    "handler": {
      "type": "StaticResponseHandler",
      "config": {
        "status": 200,
        "headers": {
          "Content-Type": [ "text/plain; charset=UTF-8" ]
        },
        "entity": "Hello world!"
      }
    },
    "condition": "${find(request.uri.path, '^/hello')}"
  }